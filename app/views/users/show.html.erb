<div class="col-md-12">
  <div class="group">
    <%= link_to("New Meetup", new_user_meetup_path(@user), class: "btn btn-primary pull-right")%>
    <!-- <%= link_to("Edit", edit_user_path(@user), class: "btn btn-primary pull-right")%> -->
  </div>
  <h2><%= @user.name %></h2>
  <p><%= @user.email %></p>
</div>



<table class="table">
  <thead>
    <tr>
      <th>Meet up Name</th>
      <th>Comment</th>
    </tr>
  </thead>
  <tbody>
    <% @meetups.each_with_index do |meetup, index| %>
      <tr>
        <td> <%= meetup.name %></td>
        <td><%= meetup.comment %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back', users_path, class: "btn btn-default" %>
